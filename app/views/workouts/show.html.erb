<%= link_to ' Home ', profile_path(@current_user.id) %>   |
<%= link_to ' Logout', login_path, method: :delete %>

<h3><strong><%= @current_user.name%> | <%= image_tag @current_user.avatar %></strong></h3>
<br>

<div class="workouts">
  <h4>Your Previous Workouts:</h4>
    <ul>
      <% @current_user.workouts.each do |w| %>
        <li><%= link_to w.workout_name, w %></li>
        <li><%= link_to w.date, w %></li><br>
        <% end %>
    </ul>

</div>
