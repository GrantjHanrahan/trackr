<h1>Login</h1>
<!-- <p>Find me in app/views/session/create.html.erb</p> -->
<%= flash[:error] %>
<div class="user">
<%= form_tag login_path do %>

  <%#= label_tag :name %>
  <%#= text_field_tag :name, nil %>

  <%= label_tag :email %>
  <%= email_field_tag :email, nil %>

  <%= label_tag :password %>
  <%= password_field_tag :password, nil %>

  <%= submit_tag 'Login' %>

<% end %>
</div>
